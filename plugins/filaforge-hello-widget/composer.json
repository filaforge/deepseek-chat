{
    "name": "filaforge/hello-widget",
    "description": "Example Filament panel plugin: Hello Widget",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "filament/filament": "^4.0",
        "illuminate/support": "^12.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "autoload": {
        "psr-4": {
            "Filaforge\\HelloWidget\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Filaforge\\HelloWidget\\Providers\\HelloWidgetServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
